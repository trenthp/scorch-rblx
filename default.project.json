        "FogColor": [0.15, 0.15, 0.2],
        "FogEnd": 200,
        "FogStart": 50
      },
      "Atmosphere": {
        "$className": "Atmosphere",
        "$properties": {
          "Density": 0.4,
          "Offset": 0.25,
          "Color": [0.6, 0.6, 0.7],
          "Decay": [0.85, 0.85, 0.9],
          "Glare": 0,
          "Haze": 2
        }
      }
    },
    "SoundService": {
      "$className": "SoundService",
      "Sounds": {
        "$className": "Folder"
      }
    },
    "Teams": {
      "$className": "Teams",
      "Seekers": {
        "$className": "Team",
        "$properties": {
          "TeamColor": { "BrickColor": 21 },
          "AutoAssignable": false
        }
      },
      "Runners": {
        "$className": "Team",
        "$properties": {
          "TeamColor": { "BrickColor": 23 },
          "AutoAssignable": false
        }
      },
      "Lobby": {
        "$className": "Team",
        "$properties": {
          "TeamColor": { "BrickColor": 194 },
          "AutoAssignable": true
        }
      }
    }
  }
